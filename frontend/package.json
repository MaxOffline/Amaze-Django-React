    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build &&  rm -R ../staticfiles/js && rm -R ../staticfiles/css && cp -r build/static/js ../staticfiles/ && cp -r build/static/css ../staticfiles/ && rm -R ../index.html && cp -r build/index.html ../",
